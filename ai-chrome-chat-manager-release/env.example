# AI Chrome Chat Manager - Konfigürasyon Dosyası
# Bu dosyayı .env olarak kopyalayın ve API anahtarlarınızı ekleyin

# Güvenlik
CONFIG_PASSWORD=your-secure-password-here

# Gemini API Konfigürasyonu - Proje Yöneticisi
GEMINI_PM_API_KEY=your-gemini-api-key-here
GEMINI_PM_MODEL=gemini-2.0-flash
GEMINI_PM_MAX_TOKENS=2000
GEMINI_PM_TEMPERATURE=0.7

# Gemini API Konfigürasyonu - Lead Developer
GEMINI_LD_API_KEY=your-second-gemini-api-key-here
GEMINI_LD_MODEL=gemini-2.0-flash
GEMINI_LD_MAX_TOKENS=2000
GEMINI_LD_TEMPERATURE=0.9

# OpenAI API Konfigürasyonu - Proje Yöneticisi (Opsiyonel)
OPENAI_PM_API_KEY=your-openai-api-key-here
OPENAI_PM_MODEL=gpt-4o-mini
OPENAI_PM_MAX_TOKENS=2000
OPENAI_PM_TEMPERATURE=0.7

# OpenAI API Konfigürasyonu - Lead Developer (Opsiyonel)
OPENAI_LD_API_KEY=your-openai-api-key-here
OPENAI_LD_MODEL=gpt-4o-mini
OPENAI_LD_MAX_TOKENS=2000
OPENAI_LD_TEMPERATURE=0.9

# Web UI Konfigürasyonu
WEB_HOST=127.0.0.1
WEB_PORT=5000

# Memory Bank Konfigürasyonu
MEMORY_BANK_PATH=./memory-bank
PROJECT_GOAL=İki AI arasında akıllı köprü sistemi

# MCP API Keys (Opsiyonel)
NOTION_API_KEY=your-notion-api-key
FIGMA_API_TOKEN=your-figma-token
BROWSERBASE_API_KEY=your-browserbase-key 